{
    "name": "respinar/contao-plyr-bundle",
    "type": "contao-bundle",
    "require": {
        "php": "^8.0",
        "contao/core-bundle": "^4.13 || ^5.2"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Hamid Peywasti",
            "email": "hamid@respinar.com"
        }
    ],
    "require-dev": {
        "contao/manager-plugin": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Respinar\\ContaoPlyrBundle\\": "src/",
            "classmap": [
                "contao/"
            ],
            "exclude-from-classmap": [
                "contao/config/",
                "contao/dca/",
                "contao/languages/",
                "contao/templates/"
            ]
        }
    },
    "extra": {
        "contao-manager-plugin": "Respinar\\ContaoPlyr\\ContaoManager\\Plugin"
    }
}
